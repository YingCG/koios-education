{
  "name": "koios-education",
  "version": "0.0.1",
  "description": "The Koios Education Website",
  "main": "index.js",
  "scripts": {
    "start": "node server",
    "build": "run-p build:client build:server",
    "build:client": "npm run webpack -- --mode=production",
    "build:server": "npm run knex migrate:latest",
    "webpack": "webpack --config client/webpack.config.js",
    "dev": "run-p client server",
    "client": "webpack --watch --config client/webpack.config.js",
    "server": "nodemon server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "test": "jest --watchAll --verbose",
    "lint": "eslint . --ext .js,.jsx"
  },
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julianne-mccoun/koios.git"
  },
  "author": "Julianne McCoun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/julianne-mccoun/koios/issues"
  },
  "homepage": "https://github.com/julianne-mccoun/koios#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-regenerator",
      "@babel/plugin-transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@react-spring/web": "*",
    "@use-gesture/react": "^10.2.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "hash-string": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "knex": "^0.95.6",
    "node-sass": "^6.0.1",
    "pg": "^8.7.1",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-spring": "^9.3.2",
    "react-use-gesture": "^9.1.3",
    "redux": "^4.1.1",
    "sqlite3": "^5.0.1",
    "styled-components": "^5.3.1",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-regenerator": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.14",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.7.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "jest": "^26.6.3",
    "knex": "^0.95.4",
    "mini-css-extract-plugin": "^1.6.2",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "sqlite3": "^5.0.2",
    "style-loader": "^3.0.0",
    "styled-components": "^5.3.3",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.6.0"
  }
}
